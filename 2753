package ksa02.exam10;
import java.util.*;
public class codeExample {
    public static void main(String[] args) {

        Scanner scanner= new Scanner(System.in);
        int year;

        year = scanner.nextInt();

        if(year % 4 == 0 && year % 100 != 0) {
            System.out.println("1");
        }
        else if(year % 400 == 0) {
            System.out.println("1");
        }
        else {
            System.out.println("0");
        }
    }
}
