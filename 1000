package ksa02.exam10;
import java.util.*;
public class codeExample { 
  public static void main(String[] args) {

    Scanner scanner = new Scanner(System.in);
    int a, b;

    a = scanner.nextInt();
    b = scanner.nextInt();
    System.out.println(a + b);

    if(a < 0) {
        System.out.println("다시 입력하세요.");
    }
    else if(b > 10) {
        System.out.println("다시 입력하세요.");
    }
    else(a <0 && b > 10) {
        System.out.println("다시 입력하세요.");
    }
  }
}
