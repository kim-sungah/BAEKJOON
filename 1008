package ksa02.exam10;
import java.util.*;
public class codeExample {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);
        double a, b;

        a = scanner.nextInt();
        b = scanner.nextInt();
        System.out.println(a / b);

        if(a < 0) {
            System.out.println("a가 0 보다 작습니다.");
        }
        else if(b > 10) {
            System.out.println("b가 10 보다 큽니다.");
        }
    }
}
